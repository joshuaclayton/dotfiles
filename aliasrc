# git
alias gbr="git remote prune origin && git branch -r"

# filesystem
alias ..="cd ../"
alias ...="cd ../.."
alias ....="cd ../../.."
alias o=$EDITOR
alias vim=nvim
alias dotf="cd $HOME/.dotfiles"
alias ls="ls -G"
alias mkdir="mkdir -p"
alias nvimconfig="cd ~/.config/nvim"

# misc.
alias timestamp="date +%Y%m%d%H%M%S"

alias cat="bat"
